;; Create the regex string for each class of keywords
(setq scilab-xcosfuns-regexp   (regexp-opt scilab-xcosfuns   'words))
(setq scilab-functions-regexp  (regexp-opt scilab-functions  'words))
(setq scilab-primitives-regexp (regexp-opt scilab-primitives 'words))
(setq scilab-commands-regexp   (regexp-opt scilab-commands   'words))
(setq scilab-variables-regexp  (regexp-opt scilab-variables  'words))

;; Clear memory
(setq scilab-xcosfuns   nil)
(setq scilab-functions  nil)
(setq scilab-primitives nil)
(setq scilab-commands   nil)
(setq scilab-variables  nil)

;; Syntax table
(defvar scilab-syntax-table nil)
(setq scilab-syntax-table
    (let ((synTable (make-syntax-table)))

    ;; Bash style comment: "//"
    (modify-syntax-entry ?\/ ". 12b" synTable)
    (modify-syntax-entry ?\n "> b"   synTable)
    (modify-syntax-entry ?_  "w"     synTable)
    (modify-syntax-entry ?!  "w"     synTable)
    synTable))

;; Create the list for font-lock: each class of keywords is given a particular face
(setq scilab-font-lock-keywords
   `(
    (,scilab-xcosfuns-regexp   . font-lock-function-name-face)
    (,scilab-functions-regexp  . font-lock-builtin-face)
    (,scilab-primitives-regexp . font-lock-keyword-face)
    (,scilab-commands-regexp   . font-lock-type-face)
    (,scilab-variables-regexp  . font-lock-constant-face)
))

;; Define the mode
(define-derived-mode scilab-mode fundamental-mode
    "scilab-mode"
    "Major mode for editing Scilab files"
    :syntax-table scilab-syntax-table
    ;; Code for syntax highlighting
    (setq font-lock-defaults '((scilab-font-lock-keywords)))

    ;; Clear memory
    (setq scilab-xcosfuns-regexp   nil)
    (setq scilab-functions-regexp  nil)
    (setq scilab-primitives-regexp nil)
    (setq scilab-commands-regexp   nil)
    (setq scilab-variables-regexp  nil)
)

;; Setup files ending in ".sce" to open in scilab-mode
(add-to-list 'auto-mode-alist '("\\.sce\\'" . scilab-mode))

(provide 'scilab-mode)
